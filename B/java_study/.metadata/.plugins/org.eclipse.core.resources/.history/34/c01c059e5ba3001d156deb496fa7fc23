package javaMid6.p376;

public class DriverEx {

	public static void main(String[] args) {
		Driver driver = new Driver();

		Bus bus = new Bus();
		Taxi taxi = new Taxi();
		Subway subway =  new Subway();
		
		driver.driver(bus);
		driver.driver(taxi);
		driver.driver(subway);
		//지하철이 달립니다. 
	}

}
