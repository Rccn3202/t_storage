package javaMid7.p394;

public class OuterA {
     int oa=10;
     static int soa=20;//정적필드, 클래스 멤버
     void oaM() {  oa=oa - 5;
                   soa = soa +10;
                   soaM();
     }
     static void soaM() { 
    	 //oa = oa + 5; X
    	 soa=soa -1;
    	// oaM();
     }
     
     class InnerA {
    	 int ia=20;
    //	 static int sia=30;
    	 void iaM() { 
    		 ia = ia + 10;
    		 oa = oa + 20;
    		 soa = soa + 10;
    		 oaM();
    		 soaM();
    	 }
   // 	 static void siaM() {
    		 
    	 }
    	 
     }
}
